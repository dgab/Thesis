<Window x:Class="ExcelAddIn.NetworkProperitesView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dc="clr-namespace:ExcelAddIn.Init"
             xmlns:local="clr-namespace:ExcelAddIn"
             MinWidth="550" MinHeight="400"
             Title="Network Properties">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="220"/>
            <ColumnDefinition Width="3*"/>
        </Grid.ColumnDefinitions>
        <local:LogView Grid.Column="1" x:Name="logView"/>
        <dc:InitView Grid.Column="0" Width="220"/>

    </Grid>
</Window>
